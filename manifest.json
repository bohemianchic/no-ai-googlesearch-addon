{
    "manifest_version": 2,
    "name": "No-AI Google Search Redirect",
    "version": "1.0",
    "description": "Redirects all Google search URLs to add 'udm=14'.",
    "icons": {
      "48": "images.png"
  },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "*://www.google.com/*"
    ],



    "background": {
      "scripts": ["background.js"]
    },
    "applications": {
      "gecko": {
        "id": "google-udm-redirect@example.com"
      }
    }
  }
  